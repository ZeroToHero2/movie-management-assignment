{
  "name": "movie-management-assignment",
  "version": "0.0.1",
  "description": "Movie Management API",
  "author": "Bahadır Ünal",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "21.7.3",
    "npm": "10.5.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest --config ./test/jest-unit.json",
    "test:unit:cov": "jest --coverage --config ./test/jest-unit.json",
    "pretest:e2e:cov": "docker compose -f docker-compose.test.yaml up -d test-db test-redis test-rabbitmq && sleep 5",
    "test:e2e:cov": "jest --coverage --runInBand --detectOpenHandles --forceExit --config ./test/jest-e2e.json; npm run posttest:e2e:cov",
    "posttest:e2e:cov": "docker compose -f docker-compose.test.yaml stop test-db test-redis test-rabbitmq && docker compose -f docker-compose.test.yaml rm -f test-db test-redis test-rabbitmq",
    "pretest:e2e": "docker compose -f docker-compose.test.yaml up -d test-db test-redis test-rabbitmq && sleep 5",
    "test:e2e": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit --config ./test/jest-e2e.json; npm run posttest:e2e",
    "posttest:e2e": "docker compose -f docker-compose.test.yaml stop test-db test-redis test-rabbitmq && docker compose -f docker-compose.test.yaml rm -f test-db test-redis test-rabbitmq"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sendgrid/mail": "^8.1.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crpyto": "^0.0.1-security",
    "ioredis": "^5.5.0",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.4",
    "ramda": "^0.30.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "pg": "^8.13.3",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}